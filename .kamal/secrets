eval $(bin/rails runner -e production 'puts "export KAMAL_REGISTRY_PASSWORD=#{Rails.application.credentials.kamal_registry_password}"')
eval $(bin/rails runner -e production 'puts "export RAILS_MASTER_KEY=#{Rails.application.credentials.rails_master_key}"')

KAMAL_REGISTRY_PASSWORD=$KAMAL_REGISTRY_PASSWORD
RAILS_MASTER_KEY=$RAILS_MASTER_KEY
